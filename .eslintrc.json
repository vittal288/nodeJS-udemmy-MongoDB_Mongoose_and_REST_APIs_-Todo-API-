{
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "rules": {
        "semi": 2,
        "eslint linebreak-style": ["error", "unix"]
    },
    "env": {        
        "node": true
    },
    "root": true
}

